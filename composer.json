{
  "name": "bektemir/rate-limiter",
  "version": "1.1.0",
  "description": "A rate limiter library for Symfony applications with Redis support.",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Bektemir",
      "email": "bektemirkumarbayuulu@gmail.com"
    }
  ],
  "require": {
    "php": "^8.0",
    "ext-redis": "*"
  },
  "autoload": {
    "psr-4": {
      "RateLimiter\\RateLimiter\\": "src/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0"
  },
  "autoload-dev": {
    "psr-4": {
      "RateLimiter\\RateLimiter\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "phpunit --configuration phpunit.xml"
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
